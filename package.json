{
  "name": "code-story",
  "version": "0.1.0",
  "description": "A system to convert codebases into richly-linked knowledge graphs with natural-language summaries",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest run --environment jsdom --passWithNoTests",
    "test:watch": "vitest --environment jsdom",
    "test:coverage": "vitest run --coverage --environment jsdom",
    "demo": "node gui/demo/run_demo.js"
  },
  "dependencies": {
    "@mantine/core": "^7.5.0",
    "@mantine/form": "^7.5.0",
    "@mantine/hooks": "^7.5.0",
    "@reduxjs/toolkit": "^2.2.1",
    "@tabler/icons-react": "^3.0.0",
    "axios": "^1.6.7",
    "force-graph": "^1.43.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "three": "^0.162.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/three": "^0.162.0",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.1.5",
    "playwright": "^1.52.0",
    "playwright-core": "^1.52.0",
    "fsevents": "^2.3.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.1.6",
    "vitest": "^1.3.1"
  }
}
