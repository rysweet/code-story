{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "neo4jPassword": {
      "value": "${NEO4J_PASSWORD}"
    },
    "serviceImage": {
      "value": "${SERVICE_IMAGE_NAME}"
    },
    "workerImage": {
      "value": "${WORKER_IMAGE_NAME}"
    },
    "mcpImage": {
      "value": "${MCP_IMAGE_NAME}"
    },
    "guiImage": {
      "value": "${GUI_IMAGE_NAME}"
    },
    "openaiApiKey": {
      "value": "${OPENAI_API_KEY}"
    },
    "azureOpenaiApiKey": {
      "value": "${AZURE_OPENAI_API_KEY}"
    },
    "mcpClientId": {
      "value": "${MCP_CLIENT_ID}"
    },
    "mcpClientSecret": {
      "value": "${MCP_CLIENT_SECRET}"
    },
    "authEnabled": {
      "value": "${AUTH_ENABLED=false}"
    }
  }
}